
<div class="card-body">
  <%= form_with(model: [:admin, business_type], local:true ) do |form| %>
    <% if business_type.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(business_type.errors.count, "error") %> prohibited this business_type from being saved:</h2>

      <ul>
        <% business_type.errors.full_messages.each do |message| %>
        <li><%= message %></li>
        <% end %>
      </ul>
    </div>
    <% end %>
    <div class="form-group row">
      <%= form.label :name,  class: "col-lg-3 col-form-label form-control-label" %>
      <div class="col-lg-9">
        <%= form.text_field :name, id: :business_type_name,class: "form-control " %>
      </div>
    </div>
    <div class="form-group row">
      <%= form.label :business_icon,class: "col-lg-3 col-form-label form-control-label" %>
      <div class="col-lg-9">
        <%= form.text_field :business_icon, id: :business_type_business_icon, class: "form-control "  %>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-lg-3 col-form-label form-control-label"></label>
      <div class="col-lg-9">
        <%= link_to 'Back', admin_business_types_path, class: "btn btn-secondary" %>
        <%= form.submit class: "btn btn-primary", value: "Submit" %>
      </div>
    </div>
  <% end %>
</div>
      
     


