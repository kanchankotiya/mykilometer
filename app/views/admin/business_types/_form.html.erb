<div class="card-body">
  <%= form_with(model: [:admin, business_type], local:true ) do |form| %>

    <div class="form-group row">
      <%= form.label :name,  class: "col-lg-3 col-form-label form-control-label" %>
      <div class="col-lg-9">
        <%= form.text_field :name, id: :business_type_name,class: "form-control",required:true %>
      </div>
    </div>
    <div class="form-group row">
      <%= form.label :business_icon,class: "col-lg-3 col-form-label form-control-label" %>
      <div class="col-lg-9">
        <%= form.text_field :business_icon, id: :business_type_business_icon, class: "form-control", required:true %>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-lg-3 col-form-label form-control-label"></label>
      <div class="col-lg-9">
        <%= link_to 'Back', admin_business_types_path, class: "btn btn-secondary" %>
        <%= form.submit class: "btn btn-primary", value: "Submit" %>
      </div>
    </div>
  <% end %>
</div>
      
     


