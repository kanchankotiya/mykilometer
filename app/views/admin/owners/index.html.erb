<div class="container">
  <p id="notice"><%= notice %></p>
  <h1>Owners</h1>
  <div class="table-responsive">
    <table class="table table-striped">
      <thead  class="table-light">
        <tr>
          <th>Name</th>
          <th>Contact1</th>
          <th>Contact2</th>
          <th>Email</th>
          <th>Address</th>
          <th>City</th>
          <th>State</th>
          <th colspan="3"></th>
        </tr>
      </thead>

      <tbody>
        <% @owners.each do |owner| %>
          <tr>
            <td><%= owner.name %></td>
            <td><%= owner.contact1 %></td>
            <td><%= owner.contact2 %></td>
            <td><%= owner.email %></td>
            <td><%= owner.address %></td>
            <td><%= owner.city %></td>
            <td><%= owner.state %></td>
            <td><%= link_to 'Show', admin_owner_path(owner) %></td>
            <td><%= link_to 'Edit', edit_admin_owner_path(owner) %></td>
            <td><%= link_to 'Destroy', admin_owner_path(owner), method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>  
  <%= link_to 'New Owner ', new_admin_owner_path, class: "btn btn-outline-primary"  %>
</div>  
